{
    "name": "usmanbarefoot/laravel-dompdf",
    "description": "A DOMPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "dompdf", "pdf"],
    "authors": [
        {
            "name": "usmanbarefoot",
            "email": "usman.jadoon@barefoot-studio.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x",
        "dompdf/dompdf": "0.8"
    },

    "autoload": {
        "psr-4": {
            "Usmanbarefoot\\DomPDF\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        },
        "laravel": {
            "providers": [
                "Usmanbarefoot\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Usmanbarefoot\\DomPDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev-master"
}
